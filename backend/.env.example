# 数据库配置
POSTGRES_SERVER=localhost
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_DB=subtransai

# 安全配置
SECRET_KEY=your-secret-key-here
ACCESS_TOKEN_EXPIRE_MINUTES=10080  # 7 days

# API 配置
RATE_LIMIT_PER_SECOND=50

# 文件存储路径
DATA_DIR=data
UPLOAD_DIR=data/uploads
CHUNKS_DIR=data/chunks
RESULTS_DIR=data/results

# 下载 URL 配置
DOWNLOAD_BASE_URL=http://localhost:8000/api/v1/files

# DeepSeek API 配置
DEEPSEEK_API_KEY=your-deepseek-api-key
DEEPSEEK_API_URL=https://api.deepseek.com/v1/chat/completions

# Mistral API 配置
MISTRAL_API_KEY=your-mistral-api-key
MISTRAL_API_URL=https://api.mistral.ai/v1/chat/completions

# 敏感词配置
# SENSITIVE_WORDS_OSS_URL=https://example.com/sensitive-words.txt
# SENSITIVE_WORDS_UPDATE_INTERVAL=3600

# 智能体配置
AGNO_CHUNK_SIZE=100
AGNO_DEFAULT_ENGINE=mistral
AGNO_URL_EXPIRY_HOURS=24
AGNO_MAX_RETRIES=3
AGNO_RETRY_DELAY=5

# 各智能体模型配置
AGNO_VALIDATOR_ENGINE=mistral
AGNO_SPLITTER_ENGINE=mistral
AGNO_TRANSLATOR_ENGINE=mistral
AGNO_REASSEMBLER_ENGINE=mistral
AGNO_NOTIFICATION_ENGINE=mistral
